{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:allow-app-write",
    "fs:allow-home-write",
    "fs:allow-watch",
    "core:window:allow-destroy"
  ]
}
